    double height = 10.0;
    double volume = PI * radius * radius *
  height; // calculate the volume
      // Display the result
      cout << "Given: \n" ;
      cout << "Radius = " << radius << " units\n";
      cout << "Height = " << height << " units\n";
      cout << "The volume of the cylinder is: " <<
